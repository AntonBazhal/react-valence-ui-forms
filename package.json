{
  "name": "react-vui-forms",
  "version": "0.1.2",
  "description": "React component for forms inlcuding validation",
  "main": "index.js",
  "scripts": {
    "build:css": "node-sass --output-style expanded src/forms.scss src/forms.css",
    "build": "npm run build:css",
    "test:lint:js": "eslint --ext .js --ext .jsx ./src",
    "test:unit": "jest",
    "test": "npm run build && npm run test:lint:js && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/react-valence-ui-forms.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui",
    "react",
    "forms",
    "validation"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/react-valence-ui-forms/issues"
  },
  "homepage": "https://github.com/Brightspace/react-valence-ui-forms",
  "dependencies": {
    "classnames": "^2.1.3",
    "eslint": "^1.2.0",
    "eslint-config-brightspace": "^0.0.2",
    "object-assign": "^4.0.1",
    "q": "^1.2.0",
    "react": "^0.13.3",
    "react-frau-events": "^0.1.0",
    "vui-validation": "^0.3.0"
  },
  "devDependencies": {
    "jest-cli": "^0.5.8",
    "node-sass": "^3.3.3",
    "react-tools": "^0.13.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testDirectoryName": "test",
    "unmockedModulePathPatterns": [
      "react"
    ]
  }
}
